{
  "parser": "@babel/eslint-parser",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "globals": {
  },
  "rules": {
    "func-names": "off",
    "max-len": "off",
    "no-else-return": "off",
    "no-multiple-empty-lines": "off",
    "no-underscore-dangle": "off",
    "prefer-arrow-callback": "off",
    "prefer-template": "off"
  }
}
